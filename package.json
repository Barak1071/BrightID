{
  "name": "brightid",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "husky": "^1.0.0-rc.9",
    "lint-staged": "^7.1.3",
    "prettier": "^1.13.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "BrightID-rn/src/**/*.{js,json,css,md}": [
