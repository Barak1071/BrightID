{
  "name": "brightid",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "precommit": "lint-staged",
    "gulp": "gulp",
    "prettify": "prettier --write ./brightID-expo/src/**/*.js && prettier --write ./brightID-rn/src/**/*.js"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-cli": "^2.0.1",
    "gulp-diff": "^1.0.0",
    "gulp-plumber": "^1.2.0",
    "husky": "^1.0.0-rc.9",
    "lint-staged": "^7.1.3",
    "prettier": "^1.13.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "brightID-expo/src/**/*.{js,json,css,md}": [
      "prettier --write",
      "git add -A"
    ],
    "brightID-rn/src/**/*.{js,json,css,md}": [
      "prettier --write",
      "git add -A"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BrightID/BrightID"
  }
}